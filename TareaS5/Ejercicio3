using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        List<int> numerosGanadores = new List<int>();
        Console.WriteLine("Ingrese los números ganadores de la lotería primitiva (ingrese 6 números):");

        for (int i = 0; i < 6; i++)
        {
            Console.Write($"Número {i + 1}: ");
            numerosGanadores.Add(int.Parse(Console.ReadLine()));
        }

        numerosGanadores.Sort();
        Console.WriteLine("Números ganadores ordenados de menor a mayor:");
        Console.WriteLine(string.Join(", ", numerosGanadores));
    }
}
